{
  "name": "@tremolo-ui/functions",
  "version": "0.1.4",
  "description": "functions used in @tremolo-ui/react",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "build:package": "tsc && tsdown",
    "watch:package": "tsdown --watch",
    "clean": "npm run clean:build",
    "clean:build": "rimraf ./dist",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --write ."
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "mimoz",
    "email": "contact.m1m0zzz@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/m1m0zzz/tremolo-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m1m0zzz/tremolo-ui.git",
    "directory": "packages/functions"
  },
  "keywords": [
    "web-audio",
    "math",
    "midi",
    "dsp"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  }
}
