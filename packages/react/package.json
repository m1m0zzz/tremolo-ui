{
  "name": "@tremolo-ui/react",
  "version": "0.1.5",
  "description": "React component library for audio app",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "scripts": {
    "sb": "storybook dev -p 6006",
    "typecheck": "tsc",
    "build:sb": "storybook build",
    "serve": "http-server ./storybook-static",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --write .",
    "test": "jest",
    "build:package": "tsc && tsdown",
    "clean": "npm run clean:build && npm run clean:sb",
    "clean:build": "rimraf ./dist",
    "clean:sb": "rimraf ./storybook-static"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.7",
    "@storybook/addon-essentials": "^8.6.7",
    "@storybook/addon-interactions": "^8.6.7",
    "@storybook/addon-links": "^8.6.7",
    "@storybook/blocks": "^8.6.7",
    "@storybook/react": "^8.6.7",
    "@storybook/react-vite": "^8.6.7",
    "@storybook/test": "^8.6.7",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "jest-environment-jsdom": "^29.7.0",
    "jotai": "^2.12.1",
    "react": "^19",
    "react-dom": "^19",
    "storybook": "^8.6.7",
    "tone": "^15"
  },
  "peerDependencies": {
    "react": "^18 ||^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@tremolo-ui/functions": "^0.1.5",
    "clsx": "^2.1.1",
    "zustand": "^5.0.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "mimoz",
    "email": "contact.m1m0zzz@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/m1m0zzz/tremolo-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m1m0zzz/tremolo-ui.git",
    "directory": "packages/react"
  },
  "keywords": [
    "react",
    "react-components",
    "ui",
    "components",
    "web-audio"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles/index.css": "./dist/index.css",
    "./styles/global.css": "./src/styles/global.css",
    "./styles/Knob.css": "./src/components/Knob/index.css",
    "./styles/NumberInput.css": "./src/components/NumberInput/index.css",
    "./styles/Piano.css": "./src/components/Piano/index.css",
    "./styles/XYPad.css": "./src/components/XYPad/index.css"
  }
}
